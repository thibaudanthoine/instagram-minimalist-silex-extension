{
    "name": "thibaudanthoine/instagram-minimalist-silex-extension",
    "type": "library",
    "keywords": ["instagram", "silex", "extension", "twig"],
    "description": "Instagram minimalist silex extension",
    "license": "MIT",

    "autoload": {
        "psr-0": {
            "Thibaud\\": "src/"
        }
    },

    "require": {
        "php": ">=5.3.0",
        "silex/silex": ">=1.0,<2.0",
        "twig/twig": ">=1.8,<2.0-dev",
        "twig/extensions": "1.0.*@dev",
        "symfony/twig-bridge": "~2.3",
        "guzzle/guzzle": "~3.7"
    },

    "minimum-stability": "stable",

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}